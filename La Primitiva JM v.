package collection;

import java.util.*;
import javax.swing.*;

public class LaPrimitiva {

	public static void main(String[] args) {
		
		int numeros=(int)(Math.random()*49);
		
		String apuesta=JOptionPane.showInputDialog("Introduce tu apuesta");
		
		int apuestaPrimitiva=Integer.parseInt(apuesta);
			
			int numero=0;
			
			int jugadas=0;
			
			while(numero!=numeros) {
				jugadas++;
				
				System.out.println(apuestaPrimitiva);
				
			}
			
			
			
			
		
		

	}

}
